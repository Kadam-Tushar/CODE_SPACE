<h2>CODE HERE <input type="submit" name="button" class="btn btn-success" value="RUN">

<select id="selectoption" name="langname">

<%
var options = [ "c", "java", "cpp", "c99", "php", "python2","python3","go","scala","sql","vbn" ];
for ( var i = 0; i < options.length; i++ )
{
    var selected = ( languagename == options[i] ) ? "selected" : "";
    %><option value="<%=options[ i ] %>" <%=selected %>><%=options[i] %></option><%
}
%>
</select>



</select>


</h2>
<textarea name="inputarea" class="editor" row="50000" col="5000" style="top: 10%;right: 0;bottom: 0;left: 0;height:400px;width: 45%;"><%= stringname %>
 </textarea>
 
<textarea name="outputarea" row="50000" col="5000" style="top: 10%;
right: 0;
bottom: 0;
left: 0;
height:400px;
width: 45%;padding-left: 5px;background-color: black;
  color: white;">OUTPUT :<%=bodyoutput %></textarea>
